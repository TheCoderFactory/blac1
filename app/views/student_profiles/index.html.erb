<h1>Listing student_profiles</h1>

<table class="table">
    <tr>
      <th>First_name</th>
      <th>Last_name</th>
      <th>Profile_pic</th>
      <th>Bio</th>
        <th colspan="3"></th>
    </tr>

  <%= content_tag_for(:tr, @student_profiles) do |student_profile| %>
    <td><%= student_profile.first_name %></td>
    <td><%= student_profile.last_name %></td>
    <td><%= student_profile.profile_pic %></td>
    <td><%= student_profile.bio %></td>
      <td><%= link_to 'Show', student_profile %></td>
    <td><%= link_to 'Edit', edit_student_profile_path(student_profile), class: 'btn btn-edit' %></td>
    <td><%= link_to 'Delete', student_profile, method: :delete, data: { confirm: 'Are you sure?' }, class: 'btn btn-destroy' %></td>
  <% end %>
</table>


<div class="form-actions">
  <%= link_to 'New Student profile', new_student_profile_path, class: 'btn btn-success' %>
</div>
